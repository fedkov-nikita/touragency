@model IEnumerable<Testovoe_zadaniye.Models.Guide>
@{
    Layout = null;
}
<!DOCTYPE html>

<html>
<head>
    <title>Tourists List</title>
</head>
<body>
    <h3>Choose guide</h3>
    <table>
        <tr>
            <td>Name</td>
            <td>Tourists</td>
            <td></td>
        </tr>
        @foreach (var guide in Model)
        {
    <tr>
        <td>@guide.Name</td>
        <td><a href="~/Navigation/ToTouristList/@guide.GuideId">Tourists</a></td>
    </tr>
        }
    </table>
</body>
</html>