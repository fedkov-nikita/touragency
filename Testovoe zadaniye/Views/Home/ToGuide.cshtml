@model IEnumerable<Testovoe_zadaniye.DataBase.Guide>
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <title>Guide</title>
</head>
<body>
    <h3>Guide</h3>
    <table>
        <tr>
            <td>Name</td>
            <td>Guide ID</td>
            <td></td>
        </tr>
        @foreach (var guide in Model)
        {
            <tr>
                <td>@guide.Name</td>
                <td>@guide.GuideId</td>
            </tr>
        }
    </table>
</body>
</html>